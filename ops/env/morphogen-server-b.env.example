# Morphogen Server B (prod)
MORPHOGEN_SERVER_B_ENV=prod
MORPHOGEN_SERVER_B_BIND_ADDR=0.0.0.0:3000

# Provide real matrix data in production.
MORPHOGEN_SERVER_B_MATRIX_FILE=/var/lib/morphogen/matrix.bin

# Use different keys from Server A.
# Generate with: openssl rand -hex 16 | sed 's/^/0x/'
MORPHOGEN_SERVER_B_PAGE_PRG_KEY_0=REPLACE_WITH_RANDOM_16_BYTE_HEX
MORPHOGEN_SERVER_B_PAGE_PRG_KEY_1=REPLACE_WITH_RANDOM_16_BYTE_HEX

# Optional tuning.
MORPHOGEN_SERVER_B_MAX_CONCURRENT_SCANS=32
MORPHOGEN_SERVER_B_MERGE_INTERVAL_MS=1000
