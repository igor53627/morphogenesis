# Compose deployment env (production)
# Copy to a secure path (for example: /secure/path/morphogen-prod.env) and never commit real values.

# Immutable image references (tag or digest).
MORPHOGEN_SERVER_IMAGE=ghcr.io/your-org/morphogen-server@sha256:REPLACE_ME
MORPHOGEN_RPC_ADAPTER_IMAGE=ghcr.io/your-org/morphogen-rpc-adapter@sha256:REPLACE_ME

# Adapter endpoints.
UPSTREAM_RPC_URL=https://your-upstream-rpc.example
DICT_URL=https://your-dictionary-endpoint.example/mainnet_compact.dict
CAS_URL=https://your-cas-endpoint.example/cas

# Keep host exposure local by default.
RPC_BIND_HOST=127.0.0.1

# Required PIR page keys for server A/B.
MORPHOGEN_SERVER_A_PAGE_PRG_KEY_0=REPLACE_WITH_RANDOM_16_BYTE_HEX
MORPHOGEN_SERVER_A_PAGE_PRG_KEY_1=REPLACE_WITH_RANDOM_16_BYTE_HEX
MORPHOGEN_SERVER_B_PAGE_PRG_KEY_0=REPLACE_WITH_RANDOM_16_BYTE_HEX
MORPHOGEN_SERVER_B_PAGE_PRG_KEY_1=REPLACE_WITH_RANDOM_16_BYTE_HEX

# Optional server overrides.
MORPHOGEN_SERVER_A_ENV=prod
MORPHOGEN_SERVER_B_ENV=prod
MORPHOGEN_SERVER_A_BIND_ADDR=0.0.0.0:3000
MORPHOGEN_SERVER_B_BIND_ADDR=0.0.0.0:3000
MORPHOGEN_SERVER_A_MATRIX_FILE=/var/lib/morphogen/matrix.bin
MORPHOGEN_SERVER_B_MATRIX_FILE=/var/lib/morphogen/matrix.bin
MORPHOGEN_SERVER_A_MAX_CONCURRENT_SCANS=32
MORPHOGEN_SERVER_B_MAX_CONCURRENT_SCANS=32
MORPHOGEN_SERVER_A_MERGE_INTERVAL_MS=1000
MORPHOGEN_SERVER_B_MERGE_INTERVAL_MS=1000

# Local code-data mode overrides:
# DICT_URL=http://code-data/mainnet_compact.dict
# CAS_URL=http://code-data/cas
